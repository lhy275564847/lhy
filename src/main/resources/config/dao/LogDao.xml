<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ocean.project.ssm.dao.LogDao">
  <resultMap id="BaseResultMap" type="Log">
    <id property="id" column="id"/>
    <result column="model" property="model"/>
    <result column="method_name" property="methodName"/>
    <result column="arguments" property="arguments"/>
    <result column="is_success" property="isSuccess"/>
    <result column="date_create" property="dateCreate"/>
    <result column="date_update" property="dateUpdate"/>
    <result column="date_delete" property="dateDelete"/>
    <result column="create_id" property="createId"/>
    <result column="update_id" property="updateId"/>
    <result column="delete_id" property="deleteId"/>
    <result column="deleted" property="deleted"/>
    <result column="version" property="version"/>
  </resultMap>

  <insert id="insert" parameterType="Log">
    insert into log (
      id ,model ,method_name ,arguments ,is_success,date_create,date_update,date_delete,
      create_id,update_id,delete_id,deleted,version)
    values (
      #{id},#{model},#{methodName},#{arguments},#{isSuccess},#{dateCreate},#{dateUpdate},#{dateDelete},
      #{createId},#{updateId},#{deleteId},#{deleted},#{version})
  </insert>
</mapper>